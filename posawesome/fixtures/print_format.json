  "html": "<style>\n    .print-format table, .print-format tr, \n    .print-format td, .print-format div, .print-format p {\n        line-height: 100%;\n        vertical-align: middle;\n    }\n    @media screen {\n        .print-format {\n            width: 3.2in;\n            padding: 0.05in;\n            min-height: 8in;\n        }\n    }\n    .print-heading h4 {\n        margin: unset;\n    }\n    h3 {\n        margin: unset;\n    }\n    .print-format td, .print-format th {\n        padding: 5px !important;\n    }\n    p.text-center {\n        font-size: 12px;\n    }\n    table.table-condensed th, table.table-condensed td {\n        font-size: 12px;\n        font-weight: bold;\n        margin: 0px 0px;\n    }\n    thead {\n        margin-top: 0;\n    }\n    table {\n        border: 1px solid black;\n    }\n    .table {\n        margin: 5px 0px;\n        margin-bottom: 0px;\n    }\n    table thead th {\n        border: 1px solid black;\n    }\n    .table thead tr th {\n        vertical-align: bottom;\n        border-bottom: 2px solid #000000;\n    }\n    .table-condensed thead tr th {\n        color: black;\n    }\n    table td {\n        border: 0px solid black;\n    }\n    table.table-condensed tr {\n        line-height: 80%;\n    }\n    .table-condensed.no-border {\n        margin-top: 0;\n    }\n</style>\n\n{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n\n<div class=\"print-heading\">\n    <div class=\"columns\">\n        <div class=\"left-column\">\n    <div class=\"text-right\" style=\"width: 75%\">\n    </div>\n        </div>\n        <div class=\"right-column\">\n            </p>\n        </div>\n    </div>\n</div>\n\n<div class=\"columns\">\n    <div class=\"left-column\">\n            <p style=\"text-align: left;\"><b>{{ doc.company }}</b><br>\n        <b>{{ _('Date') }}</b> {{ doc.get_formatted('posting_date') }}<br>\n            {% if frappe.get_list('Access Log', filters={\n                'reference_document': doc.name, \n                'file_type': 'PDF',\n                'method': 'Print'\n            })|length >= 2 %}\n                <h4 style=\"text-align: left; margin-top: 5px; margin-bottom: 5px;\">Duplicate</h4>\n            {% endif %}\n    </div>\n    <div class=\"right-column\">\n            <b>{{ _('NTN#') }}{{ doc.ntn_no }}</b><br>\n        <b>{{ _('B#') }}</b> {{ doc.name }}<br>\n        <b>{{ _('Time') }}</b> {{ doc.get_formatted('posting_time') }}<br>\n        <p><b>{{ _('User') }}</b> {{ frappe.get_doc('User', doc.owner).full_name }}</p>\n    </div>\n</div>\n\n<table class=\"table table-condensed\">\n    <thead>\n        <tr>\n            <th width=\"48%\">{{ _('Item') }}</th>\n            <th width=\"10%\" class=\"text-right\">{{ _('Qty') }}</th>\n            <th width=\"15%\" class=\"text-right\">{{ _('Rate') }}</th>\n            <th width=\"15%\" class=\"text-right\">{{ _('Disc Rs') }}</th>\n            <th width=\"20%\" class=\"text-right\">{{ _('Amount') }}</th>\n        </tr>\n    </thead>\n    <tbody>\n        {%- for item in doc.items -%}\n        <tr>\n            <td>{{ item.item_name }}</td>\n            <td class=\"text-right\">{{ item.qty }}</td>\n            <td class=\"text-right\">{{ item.price_list_rate }}</td>\n            <td class=\"text-right\">{{ item.rate }}</td>\n            <td class=\"text-right\">{{ item.amount }}</td>\n        </tr>\n        {%- endfor -%}\n    </tbody>\n</table>\n\n<div class=\"two-columns\">\n    <div>\n        {%- if doc.total -%}<p><b>{{ _('Total:') }}</b> {{ doc.get_formatted('total', doc) }}</p>{%- endif -%}\n        {%- if doc.discount_amount -%}<p><b>{{ _('Discount:') }}</b> {{ doc.get_formatted('discount_amount', doc) }}</p>{%- endif -%}\n        {%- if doc.rounded_total -%}<p><b>{{ _('Net Total:') }}</b> {{ doc.get_formatted('rounded_total', doc) }}</p>{%- endif -%}\n    </div>\n    <div>\n        {%- if doc.paid_amount -%}<p><b>{{ _('Paid:') }}</b> {{ doc.get_formatted('paid_amount', doc) }}</p>{%- endif -%}\n        {%- if doc.rounded_total -%}<p><h3>{{ _('Total: ') }}{{ doc.net_total }}</h3></p>{%- endif -%}\n        {%- if doc.change_amount -%}<p><h3>{{ _('Change: ') }}{{ doc.change_amount }}</h3></p>{%- endif -%}\n    </div>\n</div>\n\n<p>{{ doc.terms or '' }}</p>\n\n<style>\n    .two-columns {\n        display: flex;\n        justify-content: space-between;\n        margin-top: 10px;\n    }\n    .two-columns div {\n        width: 48%;\n        font-size: 12px;\n    }\n    .two-columns div p {\n        margin: 0;\n        padding: 2px 0;\n    }\n    .columns {\n        display: flex;\n        justify-content: space-between;\n    }\n    .left-column {\n        width: 55%;\n        font-size: 11px;\n    }\n    .right-column {\n        width: 40%;\n        font-size: 11px;\n        text-align: right;\n    }\n</style>\n\n<div class=\"columns\" style=\"font-size: 11px\">\n    <div class=\"text-left\">\n        <p>{{ doc.company }}{{ _(' Undertaking:I have checked goods & have no complaints in this regards') }}</p>\n        <p><b>{{ _('No Refund & No Exchange') }}</b></p>\n    </div>\n</div>\n{%- if doc.fbr_invoice_no -%}<p class=\"text-left\"><b>{{ _('FBR Invoice No.:') }}</b> {{ doc.fbr_invoice_no }}</p>{%- endif -%}\n<p class=\"text-left\"><b>{{ _('For complaint please call at UAN number within 2 hours of purchase - 0311-1419419 ') }}</b></p>\n\n<table class=\"table table-condensed no-border\">\n    <tbody>\n        <tr>\n            <td class=\"text-center\">\n                <b>{{ _('www.g2virtu.com') }}</b>\n            </td>\n        </tr>\n    </tbody>\n</table>",
